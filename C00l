local function DPRTKV_fake_script() -- ThemeButton.ThemeScript 
	local script = Instance.new('LocalScript', ThemeButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Img.Image = "https://www.roblox.com/asset-thumbnail/image?assetId="..script.Parent.Parent.ThemeID.Text.."&width=420&height=420&format=png"
		script.Parent.Parent.Parent.Parent.Settings.Img.Image = "https://www.roblox.com/asset-thumbnail/image?assetId="..script.Parent.Parent.ThemeID.Text.."&width=420&height=420&format=png"
		script.Parent.Parent.Parent.Parent.Parent.Img.Image = "https://www.roblox.com/asset-thumbnail/image?assetId="..script.Parent.Parent.ThemeID.Text.."&width=420&height=420&format=png"
	end)
end
coroutine.wrap(DPRTKV_fake_script)()
local function UBKSK_fake_script() -- TransparencyButton.TransparencyScript 
	local script = Instance.new('LocalScript', TransparencyButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Img.ImageTransparency = ""..script.Parent.Parent.ID.Text..""
		script.Parent.Parent.Parent.Parent.Transparency = ""..script.Parent.Parent.ID.Text..""
		script.Parent.Parent.Parent.Parent.Settings.Transparency = ""..script.Parent.Parent.ID.Text..""
		script.Parent.Parent.ImageTransparency = ""..script.Parent.Parent.ID.Text..""
		script.Parent.Parent.Parent.Parent.Parent.Img.ImageTransparency = ""..script.Parent.Parent.ID.Text..""
		script.Parent.Parent.Parent.Parent.Parent.Img.BackgroundTransparency = ""..script.Parent.Parent.ID.Text..""
		script.Parent.Parent.Parent.Parent.Parent.Img.Open.BackgroundTransparency = ""..script.Parent.Parent.ID.Text..""
	end)
end
coroutine.wrap(UBKSK_fake_script)()
