-- Scripts:

local function XAETTB_fake_script() -- Hax.LocalScript 
	local script = Instance.new('LocalScript', Hax)

	local Plr = game.Players.LocalPlayer
	local Mouse = Plr:GetMouse()
	local C = script.Parent
	local Ho = script.Parent.Parent.Hover
	--[[
	C.MouseEnter:connect(function(x, y) Ho.Text = "Click to begin a new animation" Ho.Size = UDim2.new(0, Ho.TextBounds.X, 0, 15) Ho.Position = UDim2.new(0, Mouse.X+10, 0, Mouse.Y) Ho.Visible = true end)
	C.MouseMoved:connect(function(x, y) Ho.Position = UDim2.new(0, Mouse.X+10, 0, Mouse.Y) end)
	C.MouseLeave:connect(function(x, y) Ho.Text = "" Ho.Visible = false end)
	]]
	
end
coroutine.wrap(XAETTB_fake_script)()
local function ADTDEU_fake_script() -- Hax.DragGui 
	local script = Instance.new('LocalScript', Hax)

	script.Parent.Draggable = true
	script.Parent.Active = true
end
coroutine.wrap(ADTDEU_fake_script)()
local function AUQWZJT_fake_script() -- Close.Open/Close 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.Parent.Img.Visible = true
	end)
	
	script.Parent.Parent.Parent.Parent.Img.Open.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Img.Visible = false
		script.Parent.Parent.Parent.Visible = true
	end)
end
coroutine.wrap(AUQWZJT_fake_script)()
local function NUYSJV_fake_script() -- DiamondBlock.FireServer 
	local script = Instance.new('LocalScript', DiamondBlock)

	local lp = game.Players.LocalPlayer
	local RS = game:GetService("ReplicatedStorage")
	
	script.Parent.MouseButton1Click:Connect(function()
		RS.SpawnDiamondBlock:FireServer(lp)
	end)
	
	script.Parent.Text = ""..script.Parent.Name..""
end
coroutine.wrap(NUYSJV_fake_script)()
local function TKRND_fake_script() -- Settings.Open/Close 
	local script = Instance.new('LocalScript', Settings)

	script.Parent.MouseButton1Click:Connect(function()
		if	script.Parent.Parent.Parent.Settings.Visible == true then
			script.Parent.Parent.Parent.Settings.Visible = false
		else
			script.Parent.Parent.Parent.Settings.Visible = true
		end
	end)
end
coroutine.wrap(TKRND_fake_script)()
